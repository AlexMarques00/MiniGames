                        print(" X | X |   ")
                        print(" O | X | O ")
                        print("Moves left for you: 'TL' and 'CR'.")
                        x = input()
                        if x == 'TL':
                            print(" O | O | X ")
                            print(" X | X |   ")
                            print(" O | X | O ")
                            print("My turn.")
                            print(" O | O | X ")
                            print(" X | X | X ")
                            print(" O | X | O ")
                            print("I win! See you next time.")
                        elif x == 'CR':
                            print("   | O | X ")
                            print(" X | X | O ")
                            print(" O | X | O ")
                            print("My turn.")
                            print(" X | O | X ")
                            print(" X | X | O ")
                            print(" O | X | O ")
                            print("It's a tie! Congratulations! It is impossible to win when I'm playing first, so be proud!")
                    elif x == 'CL':
                        print("   |   | X ")
                        print(" O | X |   ")
                        print(" O | X | O ")
                        print("My turn.")
                        print("   | X | X ")
                        print(" O | X |   ")
                        print(" O | X | O ")
                        print("I win! See you later.")
                    elif x == 'CR':
                        print("   |   | X ")
                        print("   | X | O ")
                        print(" O | X | O ")
                        print("My turn.")
                        print("   | X | X ")
                        print("   | X | O ")
                        print(" O | X | O ")
                        print("I win! See you later.")
