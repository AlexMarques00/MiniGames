print("To start, type 'hey()'")
def hey():
    print("Hey, what's your name?")
    name = input()
    print(f'Hey {name}, my name is PythonCode.')
    print("How old are you?")
    x = input()
    if int(x) < 16 and int(x) >= 0:
        print("Oh, you're just a kid. Go play some game or whatever, I hate kids!")
    elif int(x) >= 16:
        print("Cool! I was made by an 18 year old. His name is Alex Marques.")
        print("Do you like games? (Yes or No)")
        x = input()
        if x == "No" or x == 'no':
            print("Oh. Bye then.")
        elif x == "Yes" or x == 'yes':
            print("Let's play a game then.")
            print("How about tic tac toe?")
            print("I'll be the 'X'")
            print("   |   |   ")
            print("   | X |   ")
            print("   |   |   ")
            print("Where do you want your 'O'?")
            print("If you want to put it in the top left, say 'TL'.")
            print("If you want to put it in the top center, say 'TC'.")
            print("If you want to put it in the top right, say 'TR'.")
            print("If you want to put it in the center left, say 'CL'.")
            print("If you want to put it in the center right, say 'CR'.")
            print("If you want to put it in the bottom left, say 'BL'.")
            print("If you want to put it in the bottom center, say 'BC'.")
            print("If you want to put it in the bottom right, say 'BR'.")
            x = input()
            if x == 'TL':
                print(" O |   |   ")
                print("   | X |   ")
                print("   |   |   ")
                print("My turn.")
                print(" O |   | X ")
                print("   | X |   ")
                print("   |   |   ")
                print("Moves left for you: 'TC', 'CL', 'CR', 'BL', 'BC' and 'BR'")
                x = input()
                if x == 'TC':
                    print(" O | O | X ")
                    print("   | X |   ")
                    print("   |   |   ")
                    print("My turn.")
                    print(" O | O | X ")
                    print("   | X |   ")
                    print(" X |   |   ")
                    print("I win! See you next time.")
                elif x == 'CL':
                    print(" O |   | X ")
                    print(" O | X |   ")
                    print("   |   |   ")
                    print("My turn.")
