def equation(a,b,c):
    if a != 0:
        s = b ** 2 - 4*a*c
        if s < 0:
            print('S = {}')
        else:
            x = -b + (s ** 0.5)/(2 * a)
            y = -b - (s ** 0.5)/(2 * a)
            if x != y:
                print('S = {%r, %r}' %(x,y))
            else:
                print('S = {%r}' %(x))
    else:
        z = -c / b
        print('S = {%r}' %(z))
